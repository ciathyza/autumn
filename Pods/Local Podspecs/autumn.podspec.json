{
  "name": "autumn",
  "version": "1.0.0",
  "license": {
    "type": "Proprietary",
    "file": "LICENSE"
  },
  "homepage": "https://github.com/ciathyza/autumn",
  "authors": {
    "Ciathyza": "ciathyza@ciathyza.com"
  },
  "summary": "An automation framework for iOS projects utilizing XCTest.",
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "git": "https://github.com/ciathyza/autumn.git",
    "tag": "1.0.0"
  },
  "source_files": "AutumnFramework/AutumnFramework/Source/**/*.{swift,h,m}",
  "requires_arc": true,
  "weak_frameworks": "XCTest",
  "dependencies": {
    "Alamofire": [
      "~> 4.5"
    ]
  },
  "pod_target_xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "$(inherited) \"$(PLATFORM_DIR)/Developer/Library/Frameworks\""
  }
}
